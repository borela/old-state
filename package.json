{
  "name": "old-state",
  "version": "1.0.6",
  "description": "Decorator for ReactJS components that saves the old state before it gets updated.",
  "license": "Apache-2.0",
  "main": "build",
  "scripts": {
    "clean": "rimraf \"build/!(.git)\"",
    "build": "npm run clean && babel src -d build --ignore **/__*__/** -s",
    "dev": "jest src --verbose --watch",
    "start": "npm run dev",
    "test": "jest src --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borela/old-state.git"
  },
  "bugs": {
    "url": "https://github.com/borela/old-state/issues"
  },
  "homepage": "https://github.com/borela/old-state#readme",
  "keywords": [
    "decorator",
    "old",
    "state",
    "react",
    "ui"
  ],
  "author": {
    "name": "Alexandre Borela",
    "email": "alexandre@borela.tech",
    "url": "http://www.borela.tech"
  },
  "devDependencies": {
    "babel-preset-ctrine-web-lib": "^1.0.0",
    "ctrine-dev-preset": "^4.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^15.6.1"
  }
}
